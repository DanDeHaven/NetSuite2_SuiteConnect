create or replace view FINANCE.ADMIN_CORE_CURATED.CTS_NS_CLASS(
	NS_CLASS_ID,
	NS_PARENT_CLASS_ID,
	NS_SUBSIDIARY_ID,
	CLASS,
	FULL_NAME
) as
--
--  D3 5.29.24 CREATED
--
SELECT
    SD.ID AS NS_CLASS_ID,
    SD.PARENT AS NS_PARENT_CLASS_ID,
    SD.SUBSIDIARY AS NS_SUBSIDIARY_ID,
    SD.NAME AS CLASS,
    SD.FULLNAME AS FULL_NAME
    
FROM
    FINANCE.CTS_NETSUITE2.CLASSIFICATION SD

WHERE 
    SD._FIVETRAN_DELETED = 'FALSE';
